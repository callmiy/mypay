{
  "name": "burda",
  "version": "0.0.1",
  "description": "Shift work times and wages",
  "main": "src/index.js",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "test": "node config/test.js --env=jsdom",
    "deploy": "cross-env NODE_ENV=production webpack --port=4019 --config=config/webpacks/prod.js",
    "start": "cross-env NODE_ENV=development WEBPACK_DEV_SERVER_URL=http://localhost:4019 yarn run watch",
    "watch": "webpack-dev-server --port=4019 --config=config/webpacks/dev.js --colors --watch-stdin",
    "compile-handlebars-server": "node_modules/.bin/handlebars src/templates/ -f ../priv/static/offline/templates.js",
    "compile-handlebars-server-prod": "node_modules/.bin/handlebars  src/templates/ -f ../priv/static/offline/templates.js -m"
  },
  "keywords": [
    "shift",
    "work time",
    "wages",
    "elixir",
    "phoenix",
    "websocket"
  ],
  "author": "maneptha@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/dom-inputevent": "^1.0.1",
    "@types/graphql": "^0.13.4",
    "@types/jest": "^23.3.1",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.5.5",
    "@types/phoenix": "^0.0.4",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-find": "^6.3.3",
    "@types/yup": "^0.24.7",
    "autoprefixer": "^9.0.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-graphql-tag": "^1.6.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "glob-all": "^3.1.0",
    "graphql": "^0.13.2",
    "handlebars-loader": "^1.7.0",
    "image-webpack-loader": "^4.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^2.1.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-dev-utils": "6.0.0-next.3e165448",
    "resolve": "^1.8.1",
    "sass-loader": "^7.1.0",
    "semantic-ui-less": "^2.3.3",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-graphql-plugin": "^1.1.2",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "typescript-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-assets-manifest": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "babel-jest": "^23.4.2",
    "graphql-tag": "^2.9.2",
    "handlebars": "^4.0.11",
    "jest": "^23.5.0",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "phoenix": "^1.3.4",
    "pouchdb-browser": "^7.0.0",
    "pouchdb-debug": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "rxjs": "^6.2.2",
    "ts-jest": "^23.1.4",
    "tslib": "^1.9.3",
    "yup": "^0.26.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.(js|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts)$"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.js",
      "js",
      "json",
      "node",
      "mjs",
      "handlebars"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  }
}
